<idea-plugin>
    <id>com.github.mprops.idea</id>
    <name>Multiline properties format</name>
    <version>181.1.0</version>
    <vendor email="mike.fursov@gmail.com" url="https://github.com/mprops/mprops-idea">github.com/mprops/mprops-idea</vendor>

    <description><![CDATA[
      Multiline properties format.
    ]]></description>

    <idea-version since-build="181"/>

    <!--<depends>com.intellij.modules.lang</depends>-->

    <actions/>
    <!-- Plugin's application components -->
    <application-components/>


    <extensions defaultExtensionNs="com.intellij">
        <fileTypeFactory implementation="com.github.mprops.idea.MPropsFileTypeFactory"/>
        <lang.parserDefinition language="MProps" implementationClass="com.github.mprops.idea.parser.MPropsParserDefinition"/>
        <lang.syntaxHighlighterFactory language="MProps" implementationClass="com.github.mprops.idea.editor.MPropsSyntaxHighlighter$MPropsSyntaxHighlighterFactory"/>
        <!--<completion.contributor language="MProps" implementationClass="com.github.mprops.idea.editor.codecompletion.MPropsCompletionContributor"/>-->
    </extensions>
</idea-plugin>
